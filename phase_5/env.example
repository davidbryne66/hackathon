# Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Looker API Credentials
# Get from: Looker Admin → Users → Edit → API3 Keys
LOOKERSDK_BASE_URL=https://your-looker-instance.cloud.looker.com:19999
LOOKERSDK_CLIENT_ID=your_client_id_here
LOOKERSDK_CLIENT_SECRET=your_client_secret_here
LOOKERSDK_VERIFY_SSL=true
